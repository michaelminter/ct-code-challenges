<p id="notice"><%= notice %></p>

<% @timeline.each do |t| %>
  <div class="row justify-content-md-center">
    <div class="col-md-6">
      <div class="card mb-3">
        <div class="card-body">
          <p class="card-text"><%= t['text'] %></p>
        </div>
      </div>
    </div>
  </div>
<% end %>
